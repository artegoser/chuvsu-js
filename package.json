{
  "name": "chuvsu-js",
  "version": "1.1.0",
  "description": "Node.js library for ChuvSU student portal (lk.chuvsu.ru) and schedule (tt.chuvsu.ru)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./browser": {
      "import": "./dist/browser.js",
      "types": "./dist/browser.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\"",
    "build": "pnpm clean && tsc",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "chuvsu",
    "chuvash-university",
    "schedule",
    "parser"
  ],
  "author": "Artemy Egorov",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artegoser/chuvsu-js.git"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.28.1",
  "dependencies": {
    "linkedom": "^0.18.12",
    "undici": "^7.20.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.1",
    "typescript": "^5.9.3"
  }
}
